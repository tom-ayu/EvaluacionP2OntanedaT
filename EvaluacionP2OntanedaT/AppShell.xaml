<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="EvaluacionP2OntanedaT.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:EvaluacionP2OntanedaT"
    Shell.FlyoutBehavior="Flyout"
    Title="Evaluacion_P2_Tomás_Ontaneda">

    <ShellContent
        Title="Primera Página"
        ContentTemplate="{DataTemplate local:PrimeraPagina}"
        Route="PrimeraPagina" />
    <ShellContent
        Title="Segunda Página"
        ContentTemplate="{DataTemplate local:SegundaPagina}"
        Route="SegundaPagina" />

    <TabBar>
        <Tab
        Title="Primera Página" Route="PrimeraPagina"/>
        <Tab
        Title="Segunda Página" Route="SegundaPagina" />
    </TabBar>

</Shell>
